<div class="page-header">
  <h1>
    Tags restaurants
    <a href="/tags_restaurants/new" class="btn btn-lg btn-success">New Tags restaurant</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Tag</th>
        <th>Restaurant</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @tags_restaurants.each do |tags_restaurant| %>
      <tr>
        <td><%= tags_restaurant.tag_id %></td>
        <td><%= tags_restaurant.restaurant_id %></td>
        <td><%= tags_restaurant.user_id %></td>
        <td>
          <a href="/tags_restaurants/<%= tags_restaurant.id %>" class="btn btn-primary">Show</a>
          <a href="/tags_restaurants/<%= tags_restaurant.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_tags_restaurant/<%= tags_restaurant.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
